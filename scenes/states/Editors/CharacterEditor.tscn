script/source = "@tool
extends Sprite2D
var tex = Paths.texture('editors/character_editor/ground')
func _ready() -> void: 
	set_editable_instance(self,false)
	texture = tex

func _validate_property(property: Dictionary) -> void:
	match property.name:
		\"texture\": property.usage = PROPERTY_USAGE_NONE
"

